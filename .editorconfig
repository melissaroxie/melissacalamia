root=true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
tab_width = 2
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
spaces_around_operators = true
curly_bracket_next_line = false
spaces_around_brackets = both

[package.json]
indent_style = space
indent_size = 2

[*.jsx]
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false
